{
  "name": "preact-ts-rollup-starter",
  "version": "1.0.0",
  "main": "index.js",
  "module": "dist/bundle.js",
  "private": true,
  "author": "Peter van Dijk <petervandijk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run rollup:watch",
    "tsc": "tsc -p ./",
    "tsc:watch": "tsc -p ./ -w",
    "clean": "rm -rf dist/*",
    "rollup": "rollup -c rollup.config.js",
    "rollup:watch": "rollup -c rollup.config.js -w",
    "build": "npm run clean && npm run rollup && npm run build:lambda",
    "build:lambda": "netlify-lambda build src/lambda",
    "start:lambda": "netlify-lambda serve src/lambda -p 8000"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-html": "^0.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "bl": "^0.9.5",
    "dotenv": "^8.6.0",
    "levelup": "^4.4.0",
    "netlify-lambda": "^1.6.3",
    "prettier": "^1.19.1",
    "rollup": "^2.67.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-preact": "^0.5.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-workbox": "^5.2.1",
    "semver": "^4.3.6",
    "tailwindcss": "^1.9.6",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "core-js": "^3.21.1",
    "form-data": "^4.0.0",
    "immer": "^6.0.9",
    "moment": "^2.27.0",
    "preact": "^10.6.6",
    "preact-icons": "^2.2.5",
    "preact-localstorage": "^1.0.2",
    "preact-router": "^3.2.1",
    "serialize-javascript": "^2.1.2",
    "simple-zustand-devtools": "^1.0.1",
    "zustand": "^2.2.4"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "bracketSpacing": true
  },
  "browserslist": [
    "Chrome 68",
    "Edge 17",
    "Explorer 11",
    "Firefox 61",
    "iOS 11",
    "Safari 11"
  ]
}
